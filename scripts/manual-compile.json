{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./temp_js",
    "baseUrl": ".",
    "paths": {
      "~/*": ["../web/app/*"]
    },
    "types": ["node"]
  },
  "include": [
    "../web/app/services/video.server.ts",
    "../web/app/services/gemini.server.ts"
  ],
  "exclude": ["node_modules"]
}
